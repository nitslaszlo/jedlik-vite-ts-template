{
  "name": "jedlik-vite-ts-tamplate",
  "version": "0.0.1",
  "description": "Vite + Vue 3 + Vuetify 3 + Pinia + Vitest + TypeScript template",
  "keywords": [
    "vite",
    "vue3",
    "vuetify3",
    "Pinia",
    "Vitest",
    "TypeScript",
    "TSLint",
    "Prettier"
  ],
  "scripts": {
    "dev": "vite --port 8080 --open",
    "json-server": "json-server --watch db.json --routes routes.json",
    "dev-clear": "vite --force --port 8080 --open",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --ignore-path .gitignore --fix",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}",
    "test": "vitest run",
    "test-coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@mdi/font": "^6.6.96",
    "axios": "^0.26.1",
    "pinia": "^2.0.13",
    "roboto-fontface": "^0.10.0",
    "vue": "^3.2.33",
    "vue-global-api": "^0.4.1",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.14",
    "vue3-table-lite": "^1.1.9",
    "vuetify": "^3.0.0-beta.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vuetify/vite-plugin": "^1.0.0-alpha.10",
    "c8": "^7.11.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.6.2",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.34.7"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/nitslaszlo/jedlik-vite-ts-template"
  },
  "license": "MIT"
}
